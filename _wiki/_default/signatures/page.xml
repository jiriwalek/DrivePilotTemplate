<content><![CDATA[#if(!$document)
#set($document = $trackerService.getWorkItem("drivepilot","DP-76").getModule())
#end

#macro(renderVerdictIcon $verdict)
  #if($verdict.id.equals("signed"))
   <img style="height:16px" src="/polarion/ria/images/sidebars/signatures/signed.png"/>
  #elseif($verdict.id.equals("rejected"))
   <img style="height:16px" src="/polarion/ria/images/sidebars/signatures/rejected.png"/>
  #else
   <img style="height:16px" src="/polarion/ria/images/sidebars/signatures/pending.png"/>
  #end
#end

<table border=0 width="700px" cellpadding=5>
#foreach($c in $document.getSignatureContexts())
  <tr style="border-bottom: thin solid lightgray;height:40px;">
  	<td colspan=3 style="font-size:15px;vertical-align:bottom">*$c.targetStatus.name*  $!c.transitionRevision</td></tr>
  #foreach($s in $c.signatures)
    <tr style="">
      <td style="width:20px;padding-left:10px">#renderVerdictIcon($s.verdict)</td>
      <td align="left"> *$s.user.name* <br/> <span style="font-size:11px;color:gray">Product Manager</span></td>
      <td align="left" style="width:200px; vertical-align:middle;" > $s.verdict.name <br/><span style="font-size:11px;color:gray;"> 2014-07-22 2:04 $!s.verdictTime </span></td>
    </tr>
  #end
#end
</table>

<link href="http://heelhook.github.io/chardin.js/chardinjs.css" rel="stylesheet">
<script src="http://heelhook.github.io/chardin.js/chardinjs.min.js"></script>
<div class="test" id="test">

<img src="data:image/png;base64," data-intro="An awesome 18th-century painter, who found beauty in everyday, common things." data-position="right" onclick="$('.test').chardinJs('start')"/>


</div>

]]></content>
</wiki-page>